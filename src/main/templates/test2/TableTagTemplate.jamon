<%import>
	test2.model.User;
	java.util.List;
	java.io.Serializable;
</%import>
<%args>
	List<Serializable> entities;
</%args>

<table style='width: 100%;' class='smartTableUnselectable'>
	<thead>
		<tr>
			<th>Name</th>
			<th>Username</th>
		</tr>
	</thead>
	<tbody>
		<%for Serializable entity : entities %>
			<%java User user = (User) entity; %>
			<tr onmousedown="RowClick(this,false);">
				<td><% user.getName() %></td>
				<td><% user.getUsername() %></td>
			</tr>
		</%for>
	</tbody>
</table>
		